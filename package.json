    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.10",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "semantic-release-yarn": "^3.0.2",
    "typescript": "^5.3.3"
