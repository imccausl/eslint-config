    "@typescript-eslint/parser": "^6.21.0",
    "@yarnpkg/sdks": "^3.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
