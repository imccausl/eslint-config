{
  "name": "@imccausl/eslint-config",
  "version": "4.0.3",
  "packageManager": "yarn@4.9.2",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/imccausl/eslint-config.git"
  },
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "default": "./lib/index.js",
        "types": "./lib/index.d.ts"
      },
      "./base": {
        "default": "./lib/base/index.js",
        "types": "./lib/base/index.d.ts"
      },
      "./react": {
        "default": "./lib/react/index.js",
        "types": "./lib/react/index.d.ts"
      },
      "./test": {
        "default": "./lib/test/index.js",
        "types": "./lib/test/index.d.ts"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "tsc -b ./tsconfig.build.json",
    "build": "yarn clean && yarn compile",
    "prepare": "husky",
    "publish": "semantic-release",
    "lint": "yarn dev lint",
    "lint:fix": "yarn lint --fix",
    "format": "yarn dev format",
    "typecheck": "tsc --noEmit"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./base": {
      "default": "./src/base/index.ts"
    },
    "./react": {
      "default": "./src/react/index.ts"
    },
    "./test": {
      "default": "./src/test/index.ts"
    }
  },
  "dependencies": {
    "globals": "^16.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@imccausl/dev": "^1.2.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@typescript-eslint/types": "^8.36.0",
    "@typescript-eslint/utils": "^8.36.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "@yarnpkg/sdks": "^3.2.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.5.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.6",
    "semantic-release-yarn": "^3.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "peerDependencies": {
    "@eslint/js": "^9.30.1",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-x": "^4.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "typescript": ">=5.3",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/eslint-plugin-query": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    }
  }
}
