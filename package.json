{
    "name": "@imccausl/eslint-config",
    "version": "4.0.2",
    "packageManager": "yarn@4.5.3",
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/imccausl/eslint-config.git"
    },
    "type": "module",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "tag": "latest",
        "main": "./lib/index.js",
        "exports": {
            ".": {
                "default": "./library/index.js",
                "types": "./library/index.d.ts"
            },
            "./base": {
                "default": "./library/base/index.js",
                "types": "./library/base/index.d.ts"
            },
            "./react": {
                "default": "./library/react/index.js",
                "types": "./library/react/index.d.ts"
            },
            "./test": {
                "default": "./library/test/index.js",
                "types": "./library/test/index.d.ts"
            }
        }
    },
    "scripts": {
        "clean": "rm -rf lib",
        "compile": "tsc -b ./tsconfig.build.json",
        "build": "yarn clean && yarn compile",
        "prepare": "husky",
        "publish": "semantic-release",
        "lint": "eslint .",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "default": "./src/index.ts"
        },
        "./base": {
            "default": "./src/base/index.ts"
        },
        "./react": {
            "default": "./src/react/index.ts"
        },
        "./test": {
            "default": "./src/test/index.ts"
        }
    },
    "dependencies": {
        "globals": "^16.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/js": "^9.30.1",
        "@imccausl/eslint-config": "^3.1.4",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/eslint": "^9.6.1",
        "@types/node": "^20.19.4",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "@typescript-eslint/types": "^8.36.0",
        "@typescript-eslint/utils": "^8.36.0",
        "@vitest/eslint-plugin": "^1.3.4",
        "@yarnpkg/sdks": "^3.2.0",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-import-x": "^4.5.0",
        "eslint-plugin-jest": "^28.9.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-testing-library": "^7.1.1",
        "husky": "^9.1.7",
        "prettier": "^3.6.2",
        "semantic-release": "^24.2.6",
        "semantic-release-yarn": "^3.0.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.36.0"
    },
    "peerDependencies": {
        "@eslint/js": "^9.30.1",
        "@tanstack/eslint-plugin-query": "^5.62.1",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "@vitest/eslint-plugin": "^1.3.4",
        "eslint": "^9",
        "eslint-config-prettier": ">=9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import-x": "^4.5.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-testing-library": "^7.1.1",
        "typescript": ">=5.3",
        "typescript-eslint": "^8.0.0"
    },
    "peerDependenciesMeta": {
        "@tanstack/eslint-plugin-query": {
            "optional": true
        },
        "@vitest/eslint-plugin": {
            "optional": true
        },
        "eslint-plugin-jest": {
            "optional": true
        },
        "eslint-plugin-jsx-a11y": {
            "optional": true
        },
        "eslint-plugin-react": {
            "optional": true
        },
        "eslint-plugin-react-hooks": {
            "optional": true
        },
        "eslint-plugin-testing-library": {
            "optional": true
        }
    }
}
